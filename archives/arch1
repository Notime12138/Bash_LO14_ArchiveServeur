3:25

directory Exemple/Test
A drwxr-xr-x 0
B drwxr-xr-x 0
toto1 -rw-r--r-- 16 1 2
toto2 -rw-r--r-- 111 3 5
@
directory Exemple/Test/A
A1 drwxr-xr-x 0
A2 drwxr-xr-x 0
A3 drwxr-xr-x 0
toto3 -rw-r--r-- 263 8 15
@
directory Exemple/Test/A/A1
toto4 -rwxr-xr-x 276 23 17
@
directory Exemple/Test/A/A2
@
directory Exemple/Test/A/A3
@
directory Exemple/Test/B
bar -rw-r--r-- 322 40 18
@
echo "bonjour!"

